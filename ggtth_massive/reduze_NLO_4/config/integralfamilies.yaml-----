    loop_momenta: [k1]
    propagators:
      - [ k1 , mt^2]
      - [ k1+p4,0 ]
      - [ k1-p1+p4 ,0]
      - [ k1-p1-p2+p4,0 ]
      - [ k1-p1-p2+p3+p4  , mt^2]
  - name: "F3mx34"
    loop_momenta: [k1]
    propagators:
      - [ k1 , mt^2]
      - [ k1+p4,0 ]
      - [ k1-p2+p4,0 ]
      - [ k1-p1-p2+p4,0 ]
      - [ k1-p1-p2+p3+p4  , mt^2]
